{
  "name": "alexandre.chenieux",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/ciaracdb/alexandre.chenieux.git"
  },
  "scripts": {
    "typings": "typings install",
    "postinstall": "npm run typings",
    "tsc": "tsc -p tsconfig-dev.json",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "rollup": "rollup -c rollup.config.js",
    "ngc": "ngc -p .",
    "minify": "uglifyjs compiled/build.js --screw-ie8 --compress --mangle --output compiled/build.min.js",
    "gzip": "gzip-js --file compiled/build.min.js",
    "build": "npm run ngc && npm run rollup && npm run minify && npm run gzip"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/router": "3.0.1",
    "@angular/upgrade": "2.0.1",
    "angular2-in-memory-web-api": "0.0.21",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.38",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@angular/compiler": "2.0.1",
    "@angular/compiler-cli": "^0.6.3",
    "@angular/platform-server": "^2.0.1",
    "concurrently": "^3.0.0",
    "copyfiles": "^1.0.0",
    "gulp": "^3.9.1",
    "gzip-js": "^0.3.2",
    "lite-server": "^2.2.2",
    "rm-rf": "^2.0.1",
    "rollup": "^0.36.0",
    "rollup-plugin-commonjs": "^5.0.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "uglify-js": "^2.7.3"
  }
}
